{
  "name": "workmate2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "nodemon": "nodemon server.ts",
    "start": "node src/backend/index.js",
    "test": "npm run docker:up && npm run migrate:postgres && vitest",
    "migrate:postgres": "npx prisma migrate deploy",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.338.0",
    "@aws-sdk/client-s3": "^3.337.0",
    "@aws-sdk/lib-dynamodb": "^3.338.0",
    "@aws-sdk/util-dynamodb": "^3.338.0",
    "@prisma/client": "^4.15.0",
    "@types/js-cookie": "^3.0.3",
    "@types/mongoose": "^5.11.97",
    "aws-sdk": "^2.1382.0",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "dotenv-expand": "^10.0.0",
    "esbuild": "^0.17.19",
    "express": "^4.18.2",
    "firebase": "^9.22.0",
    "js-cookie": "^3.0.5",
    "json-server": "^0.17.3",
    "json2csv": "^6.0.0-alpha.2",
    "mongoose": "^7.1.1",
    "pinia": "^2.0.36",
    "ts-node": "^10.9.1",
    "vue": "^3.3.2",
    "vue-cookies": "^1.8.3",
    "vue-js-cookie": "^2.1.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.0.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "typescript": "~5.0.4",
    "vite": "^4.3.5",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.6.4"
  }
}
